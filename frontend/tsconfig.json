{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@Chatter/*": ["./src/pages/Chatter/*"],
    },
    "allowImportingTsExtensions": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src", "src/pages/Discovery/index.tsx"],
}
