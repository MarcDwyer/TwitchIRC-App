{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "lint": "deno lint && deno run -A npm:eslint src/"
  },
  "imports": {
    "@eslint/js": "npm:@eslint/js@^10.0.1",
    "eslint": "npm:eslint@^10.0.0",
    "eslint-plugin-react-hooks": "npm:eslint-plugin-react-hooks@^7.0.1",
    "react": "npm:react@^19.2.0",
    "react-dom": "npm:react-dom@^19.2.0",
    "react-dom/client": "npm:react-dom@^19.2.0/client",
    "react-resizable": "npm:react-resizable@^3.1.3",
    "react/jsx-runtime": "npm:react@^19.2.0/jsx-runtime",
    "typescript": "npm:typescript@^5.9.3",
    "typescript-eslint": "npm:typescript-eslint@^8.54.0",
    "usehooks-ts": "npm:usehooks-ts@^3.1.1",
    "vite": "npm:vite@^6.3.5",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^4.2.2",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18"
  },
  "compilerOptions": {
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["npm:@types/react@^19.2.0"]
  },
  "nodeModulesDir": "auto"
}
